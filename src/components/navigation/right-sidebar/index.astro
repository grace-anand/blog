---
const { headings } = Astro.props;
import TableOfContents from "./TableOfContents";
import generateToc from "@lib/utils/generateToc";
import ShareSection from "./ShareSection.astro";
import EditSection from "./EditSection.astro";
---
{
  headings && (
    <TableOfContents
      client:media="(min-width: 50em)"
      toc={generateToc(headings, 'Overview')}
      labels={{ onThisPage: 'Table Of Contents' }}
    />
  )
}
<ShareSection class="mt-2"/>
<EditSection class="mt-2"/>
